{
  "name": "graphql-mongo-quick",
  "version": "1.0.0",
  "description": "Quick set-up to start using GraphQL with Mongo",
  "main": "server.js",
  "engines": {
    "npm": "5.5.1",
    "node": "8.3.0"
  },
  "scripts": {
    "start": "babel-node -- server.js",
    "server": "nodemon server.js --exec babel-node --presets=env",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Conrad Knapp",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.19.0",
    "mongoose": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.14.11"
  }
}
